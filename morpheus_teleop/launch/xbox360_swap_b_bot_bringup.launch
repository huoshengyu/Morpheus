<launch>
    <arg name="robot_ip" default="192.168.1.102" />
    <arg name="reverse_ip" default="192.168.1.2" />
    <arg name="tool_device_name" default="/tmp/ttyUR" />
    <arg name="kinematics_config" default="$(find homestri_bringup)/etc/a_bot_calibration.yaml" />
    <arg name="controllers" default="joint_state_controller twist_controller speed_scaling_state_controller force_torque_sensor_controller"/>
    <arg name="stopped_controllers" default="joint_group_vel_controller scaled_pos_joint_traj_controller motion_control_handle cartesian_motion_controller cartesian_force_controller cartesian_compliance_controller"/>
    <arg name="controller_config_file" default="$(find homestri_bringup)/config/a_bot_controllers.yaml" />
    <arg name="moveit_controller_config_file" default="$(find homestri_bringup)/config/a_bot_moveit_controllers.yaml" />
    <arg name="use_rviz" default="true" />

    <arg name="xacro_path" default="$(find morpheus_teleop)/urdf/scenes/a_bot_scene.xacro"/>
    <arg name="srdf_path" default="$(find morpheus_teleop)/srdf/scenes/a_bot_scene.srdf"/>

    <arg name="twist_topic" default="/twist_controller/command" />

    <!-- launch a_bot_bringup -->
    <include file="$(find morpheus_teleop)/launch/a_bot_bringup.launch">
        <arg name="robot_ip" value="$(arg robot_ip)" />
        <arg name="reverse_ip" value="$(arg reverse_ip)" />
        <arg name="tool_device_name" value="$(arg tool_device_name)" />
        <arg name="kinematics_config" value="$(arg kinematics_config)" />
        <arg name="controllers" value="$(arg controllers)"/>
        <arg name="stopped_controllers" value="$(arg stopped_controllers)"/>
        <arg name="controller_config_file" value="$(arg controller_config_file)"/>
        <arg name="moveit_controller_config_file" value="$(arg moveit_controller_config_file)"/>
        <arg name="use_rviz" value="$(arg use_rviz)"/>

        <arg name="xacro_path" value="$(arg xacro_path)"/>
        <arg name="srdf_path" value="$(arg srdf_path)"/>
    <include/>


    <!-- launch teleop xbox360 swap -->
    <include file="$(find morpheus_teleop)/launch/xbox360_swap.launch">
        <arg name="twist_topic" value="$(arg twist_topic)" />
    <include/>

</launch>