<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="vx300s">

    <xacro:include filename="$(find morpheus_description)/urdf/macros/trossen_macro.xacro" />

    <xacro:trossen
        prefix=""
        parent="world"
    >
        <origin xyz="0 0 ${0.0254 * 34.75}" rpy="0 0 0" />
    </xacro:trossen>

    <gazebo>
        <plugin name="ros_control" filename="libgazebo_ros_control.so">
        </plugin>
    </gazebo>
</robot>